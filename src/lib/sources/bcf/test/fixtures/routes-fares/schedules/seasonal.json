ute.innerText = route.getAttribute('data-schedule-route')\n                    var directionLink = Array.from(document.querySelectorAll('.direction-link'));\n                    if(index === 0 && directionLink[1]) {\n                        directionLink[1].href = \"#ReturnSchedule\"\n                    } else if (directionLink[1]) {\n                        directionLink[1].href = \"#OnwardSchedule\"\n                    }\n                }\n            })\n\n            if(scrollPos>250){\n                element.classList.add('visible')\n            }else{\n                element.classList.remove('visible')\n            }\n        });\n    }\n    document.addEventListener('scroll', function(e) {\n        lastKnownScrollPosition = window.scrollY;\n        if (!ticking) {\n            window.requestAnimationFrame(function() {\n                onScrollScheduleStickyHeader(lastKnownScrollPosition);\n                ticking = false;\n            });\n\n            ticking = true;\n        }\n\n    });\n\n</script>\n</section><section class=\"yCmsContentSlot\">\n\n\n\n\n\n    \n        \n        \n    \n    \n\n\n<div class=\"container\">\n\t<div class=\"row offers-grouped-component\">\n\t\n\t\t<div class=\"js-list-box-width col-lg-4 col-md-4 col-sm-4 col-xs-12\">\n                            <div class=\"card media-top vacation-deal-component\"  style=\"background:#f2f2f2\" >\n      <div class=\"featured-box-main\">\n\t        </div>\n        <div class=\"p-relative\">\n        <img class='img-fluid js-responsive-image' data-media='{\"1\":\"https://www.bcferries.com/web_image/h5a/h1b/8932232560670.jpg\"}' alt='BC Ferries app' title='BC Ferries app' />\n        <div class=\"location-bx\">\n                <br />\n               </div>\n         </div>\n        <div class=\"promo-cards flex-card\">\n\t      <div class=\"card-body\">\n\t        <div class=\"deal-info\"></div>\n              <div class=\"card-title \"><h2><h3>Download the BC Ferries app </h3>\n</h2></d